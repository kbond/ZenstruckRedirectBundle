{
    "name": "zenstruck/redirect-bundle",
    "description": "Store redirects for your site and keeps statistics on redirects and 404 errors",
    "keywords": ["redirect"],
    "homepage": "https://github.com/kbond/ZenstruckRedirectBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "doctrine/doctrine-bundle": "^1.3|^2.0",
        "doctrine/orm": "^2.6",
        "symfony/framework-bundle": "^4.4|^5.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "symfony/browser-kit": "^4.4|^5.0",
        "symfony/form": "^4.4|^5.0",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/translation": "^4.4|^5.0",
        "symfony/validator": "^4.4|^5.0"
    },
    "autoload": {
        "psr-4": { "Zenstruck\\RedirectBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Zenstruck\\RedirectBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
